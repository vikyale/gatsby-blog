{
  "name": "gatsby-blog",
  "private": true,
  "description": "Blog personal de Victoria Ubaldo",
  "version": "0.1.0",
  "author": "Victoria Ubaldo",
  "dependencies": {
    "@sindresorhus/slugify": "^0.11.0",
    "gatsby": "^2.21.1",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-catch-links": "^2.3.0",
    "gatsby-plugin-feed": "^2.5.0",
    "gatsby-plugin-google-analytics": "^2.3.0",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-nprogress": "^2.3.0",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-sass": "^2.3.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-sitemap": "2.2.1",
    "gatsby-plugin-twitter": "^2.3.0",
    "gatsby-plugin-typography": "^2.5.0",
    "gatsby-remark-autolink-headers": "^2.3.0",
    "gatsby-remark-copy-linked-files": "^2.3.1",
    "gatsby-remark-images": "3.1.3",
    "gatsby-remark-prismjs": "3.3.1",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-remark": "2.6.2",
    "gatsby-transformer-sharp": "^2.5.0",
    "hygen": "^5.0.3",
    "lodash": "^4.17.13",
    "lodash.kebabcase": "^4.1.1",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "prismjs": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "sharp": "^0.30.5",
    "typography": "^0.16.19",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cli-glob": "^0.1.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.0.0",
    "write-good": "^1.0.1"
  },
  "keywords": [
    "gatsby",
    "ios",
    "frontend",
    "blog",
    "erik flores"
  ],
  "license": "MIT",
  "scripts": {
    "new:post": "hygen blog-post new --name",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "check": "npx npm-check-updates -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erikfloresq/erikfloresq.github.io"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
